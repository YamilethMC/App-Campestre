{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "20.11.1"
    },
    "preview": {
      "distribution": "internal",
      "node": "20.11.1",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "node": "20.11.1",
      "env": {
        "RCT_NEW_ARCH_ENABLED": "1"
      }
    }
  }
}
